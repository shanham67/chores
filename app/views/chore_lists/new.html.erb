<h1>New chore_list</h1>

<% form_for(@chore_list) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :plan %><br />
    <%= f.text_field :plan %>
  </p>
  <p>
    <%= f.label :worker %><br />
    <%= f.text_field :worker %>
  </p>
  <p>
    <%= f.label :duration %><br />
    <%= f.text_field :duration %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', chore_lists_path %>
